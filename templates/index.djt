{% extends 'base.html' %}
{% load static %}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block base_style%}
    {{ block.super }}
    <link rel="stylesheet" href="{% static 'index.css' %}">
    {% endblock base_style%}
    <title>Lyamma | Lyamma</title>
</head>

<body>
    {% block content %}


    <!-- Banner 1st for index starts -->
    <hr>
    <div class="index-banner">
        <h3>Special Lyamma Mo:Mo</h3>
        <h1>Delicious as fuck</h1>
        <p>Ever wanted to show your Beatboxing Skills on Swissbeatbox? Submit your Video now to the Community section
            where thousands of Beatbox fans will see it. The best videos will be featured regularly.</p>
        <button class="banner-button" style="background-color: #00182F;">
            <svg style="color: azure;" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                <path stroke-linecap="round" stroke-linejoin="round" d="M4.5 12h15m0 0l-6.75-6.75M19.5 12l-6.75 6.75">
                </path>
            </svg>

            <div class="text" style="color: azure;">
                Order
            </div>
        </button>
    </div>
    <!-- Banner 1st for index ends -->

    <!-- Filler one starts -->
    <div class="filler-container">
        <h1>Are you <span>Foodie</span>?</h1>
        <p>Lorem ipsum dolor sit, amet consectetur adipisicing elit. Doloremque, debitis.</p>
    </div>
    <!-- Filler one ends -->

    <!-- Group one Cards for index starts  -->
    <div class="cards">
        <!-- Card one starts -->
        <div class="maincard">
            <div class="card-contents">
                <h2>Chicken Pizza</h2>
                <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                <button class="banner-button card-btn">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                        stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M4.5 12h15m0 0l-6.75-6.75M19.5 12l-6.75 6.75">
                        </path>
                    </svg>
                    <div class="text">
                        Order
                    </div>
                </button>
            </div>
        </div>
        <!-- Card one ends -->

        <!-- Card two starts -->
        <div class="maincard">
            <div class="card-contents">
                <h2>Buff Momo</h2>
                <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                <button class="banner-button card-btn">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                        stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M4.5 12h15m0 0l-6.75-6.75M19.5 12l-6.75 6.75">
                        </path>
                    </svg>
                    <div class="text">
                        Order
                    </div>
                </button>
            </div>
        </div>
        <!-- Card two ends -->

        <!-- Card three starts -->
        <div class="maincard">
            <div class="card-contents">
                <h2>Veg Chowmein</h2>
                <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                <button class="banner-button card-btn">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                        stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M4.5 12h15m0 0l-6.75-6.75M19.5 12l-6.75 6.75">
                        </path>
                    </svg>
                    <div class="text">
                        Order
                    </div>
                </button>
            </div>
        </div>
        <!-- Card three ends -->

        <!-- Card four starts -->
        <div class="maincard">
            <div class="card-contents">
                <h2>Buff Chowmein</h2>
                <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                <button class="banner-button card-btn">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                        stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M4.5 12h15m0 0l-6.75-6.75M19.5 12l-6.75 6.75">
                        </path>
                    </svg>
                    <div class="text">
                        Order
                    </div>
                </button>
            </div>
        </div>
        <!-- Card four ends -->
    </div>

    <!-- Group one Cards for index ends  -->



    <!-- Banner for index starts -->
    <hr>
    <div class="index-banner">
        <h3>Special Lyamma Mo:Mo</h3>
        <h1>Delicious as fuck</h1>
        <p>Ever wanted to show your Beatboxing Skills on Swissbeatbox? Submit your Video now to the Community section
            where thousands of Beatbox fans will see it. The best videos will be featured regularly.</p>
        <button class="banner-button" style="background-color: #00182F;">
            <svg style="color: azure;" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                <path stroke-linecap="round" stroke-linejoin="round" d="M4.5 12h15m0 0l-6.75-6.75M19.5 12l-6.75 6.75">
                </path>
            </svg>

            <div class="text" style="color: azure;">
                Order
            </div>
        </button>
    </div>
    <!-- Banner for index ends -->

    <!-- Partner with us html start -->
    <!-- HTML !-->
    <div class="partner">
        <h2>Partner <span>with us </span></h2>
        <h3>List your restaurant with us and Reach several new customers through us!</h3>
        <p>Get your menu items delivered to our customer's door-step.</p>
        <button class="button-40" role="button" id="partner-btn">Send a Request</button>
    </div>


    <!-- Partner form starts -->
    <div class="overlay3" id="modalOverlay3">
        <div class="modal3">
            <span class="close" id="closeModal3">&times;</span>
            <!-- Your sign-up form goes here -->
            <div class="partner-form">
                <form action="/partnerrequest/" method="post">
                    {% csrf_token %}
                    <h2>Join <span>Us</span></h2>
                    <div class="main-combo-box">
                        <div class="div-left">
                            <label for="fullname">Fullname*</label>
                            <br>
                            <input class="combo-inputs" type="text" name="fullname" id="">
                        </div>
                        <div class="div-right">
                            <label for="fullname">Restaurant Name*</label>
                            <br>
                            <input class="combo-inputs" type="text" name="restaurantname" id="">
                        </div>
                    </div>
                    <div class="main-combo-box">
                        <div class="div-left">
                            <label for="fullname">Phone Number*</label>
                            <br>
                            <input class="combo-inputs" type="phone" name="phone" id="">
                        </div>
                        <div class="div-right">
                            <label for="fullname">Email*</label>
                            <br>
                            <input class="combo-inputs" type="email" name="email" id="">
                        </div>
                    </div>
                    
                    <textarea name="message" placeholder="Enter you message here ..." id="" cols="30" rows="8" required></textarea>
                    <input type="submit" value="Submit Request" name="" id="" class="button-79">
                </form>
            </div>
        </div>
    </div>
    <!-- Partner form ends -->



    <script src="{%static 'index.js' %}"></script>
    <!-- Partner with us html start -->
    {% endblock %}

</body>

</html>